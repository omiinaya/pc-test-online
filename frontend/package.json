{
  "name": "mmit-testing-frontend",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run generate-docs",
    "preview": "vite preview",
    "start": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "analyze": "vite-bundle-analyzer",
    "generate-docs": "typedoc --entryPointStrategy expand --out docs src/**/*.ts"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.15.0",
    "@vueuse/core": "^10.7.0",
    "chart.js": "^4.4.0",
    "ml-matrix": "^6.10.7",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.0",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.5.1",
    "web-vitals": "^3.5.0",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.41",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.17.0",
    "vite-plugin-windicss": "^1.9.0",
    "vitest": "^1.0.0",
    "vue-eslint-parser": "^9.4.0",
    "vue-tsc": "^1.8.0",
    "windicss": "^3.5.6"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
